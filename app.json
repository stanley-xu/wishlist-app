{
  "expo": {
    "owner": "taikei",
    "name": "Giftful",
    "slug": "giftful",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "giftful",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "platforms": [
      "ios"
    ],
    "ios": {
      "icon": "./assets/giftful.icon",
      "bundleIdentifier": "io.giftful",
      "associatedDomains": [
        "applinks:giftful.io",
        "applinks:www.giftful.io"
      ],
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#f7f7f7ff",
          "image": "./assets/images/splash-icon.png",
          "dark": {
            "image": "./assets/images/splash-icon.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "53212326-3deb-4f5f-8e18-8d0c0eae4c48"
      }
    }
  }
}
