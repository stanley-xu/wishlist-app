{
  "name": "wishlist-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "db:status": "npx supabase status",
    "db:seed": "npx tsx supabase/seed.ts",
    "db:reset": "npx supabase db reset && npm run db:seed",
    "db:migrate": "npx supabase migration",
    "db:push": "npx supabase db push",
    "db:generate-types": "npx supabase gen types typescript --project-id ${SUPABASE_PROJECT_ID} --schema public > supabase/database.types.ts"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "~7.3.10",
    "@react-navigation/elements": "~2.3.8",
    "@react-navigation/native": "~7.1.6",
    "@react-navigation/native-stack": "^7.6.2",
    "@supabase/supabase-js": "^2.78.0",
    "dom-to-image": "2.6.0",
    "expo": "~54.0.0",
    "expo-blur": "~15.0.7",
    "expo-constants": "~18.0.10",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-image-picker": "~17.0.8",
    "expo-linking": "~8.0.8",
    "expo-media-library": "~18.2.0",
    "expo-router": "~6.0.14",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-view-shot": "~4.0.3",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "tslib": "^2.8.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@storybook/addon-ondevice-actions": "^8.5.0",
    "@storybook/addon-ondevice-controls": "^8.5.0",
    "@storybook/react-native": "^8.5.0",
    "@types/react": "~19.1.10",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "supabase": "^2.58.5",
    "tsx": "^4.20.6",
    "typescript": "~5.9.2"
  },
  "private": true
}
