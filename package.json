{
  "name": "giftful",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "precheck": "./scripts/precheck.sh",
    "start": "npm run precheck && ENV=local expo start",
    "start:device": "npm run precheck && node scripts/update-ip.js && ENV=device expo start",
    "start:prod": "ENV=production expo start",
    "start:staging": "ENV=staging expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "npm run precheck && ENV=local expo start --android",
    "android:device": "npm run precheck && node scripts/update-ip.js && ENV=device expo start --android",
    "android:prod": "ENV=production expo start --android",
    "ios": "npm run precheck && ENV=local expo start --ios",
    "ios:device": "npm run precheck && node scripts/update-ip.js && ENV=device expo start --ios",
    "ios:prod": "ENV=production expo start --ios",
    "web": "npm run precheck && ENV=local expo start --web",
    "web:prod": "ENV=production expo start --web",
    "lint": "expo lint",
    "db:start": "npx supabase start",
    "db:restart": "npx supabase stop && npx supabase start",
    "db:status": "npx supabase status",
    "db:seed": "npx tsx supabase/seed.ts",
    "db:types": "npx supabase gen types typescript --local --schema public > supabase/database.types.ts",
    "db:types:remote": "npx supabase gen types typescript --project-id zpavrbtojxidduzdijtb --schema public > supabase/database.types.ts",
    "db:reset": "npx supabase db reset && npm run db:seed && npm run db:types",
    "db:migrate": "npx supabase migration",
    "db:push": "npx supabase db push && npm run db:types:remote",
    "generate:story": "node ./scripts/generate-story.js",
    "storybook-generate": "sb-rn-get-stories",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@hookform/resolvers": "^5.2.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/drawer": "^7.7.4",
    "@react-navigation/elements": "~2.3.8",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/native-stack": "^7.3.16",
    "@supabase/supabase-js": "^2.78.0",
    "dom-to-image": "2.6.0",
    "expo": "~54.0.25",
    "expo-blur": "~15.0.7",
    "expo-constants": "~18.0.10",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-image-picker": "~17.0.8",
    "expo-linking": "~8.0.8",
    "expo-media-library": "~18.2.0",
    "expo-router": "~6.0.14",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.8",
    "lucide-react-native": "^0.554.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.66.1",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-view-shot": "~4.0.3",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "tslib": "^2.8.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@gorhom/bottom-sheet": "^5.2.6",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/slider": "5.0.1",
    "@storybook/addon-docs": "^10.0.8",
    "@storybook/addon-ondevice-actions": "^10.0.7",
    "@storybook/addon-ondevice-controls": "^10.0.7",
    "@storybook/react-native": "^10.0.7",
    "@storybook/react-native-web-vite": "^10.0.8",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "29.5.14",
    "@types/react": "~19.1.10",
    "babel-loader": "^8.4.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-plugin-storybook": "^10.0.8",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.13",
    "react-native-svg": "15.12.1",
    "react-native-worklets": "0.5.1",
    "storybook": "^10.0.8",
    "supabase": "^2.58.5",
    "tsx": "^4.20.6",
    "typescript": "~5.9.2",
    "vite": "^7.2.2"
  },
  "private": true
}
